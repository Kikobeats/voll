{
  "name": "voll",
  "description": "Boolean expressions evaluator.",
  "homepage": "https://documentup.com/kikobeats/voll",
  "version": "1.2.3",
  "main": "index.js",
  "author": {
    "email": "josefrancisco.verdu@gmail.com",
    "name": "Kiko Beats",
    "url": "https://github.com/Kikobeats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kikobeats/voll.git"
  },
  "bugs": {
    "url": "https://github.com/Kikobeats/voll/issues"
  },
  "keywords": [
    "bool",
    "boolean",
    "evaluator",
    "expression",
    "interpreter",
    "parser"
  ],
  "dependencies": {
    "mem": "~2.0.0"
  },
  "devDependencies": {
    "coveralls": "latest",
    "git-dirty": "latest",
    "jison": "latest",
    "jison-lex": "latest",
    "json-future": "latest",
    "mocha": "latest",
    "nyc": "latest",
    "should": "latest",
    "standard": "latest"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "build": "npm run build-parser && npm run build-lexer",
    "build-lexer": "jison-lex src/lexer.y -o lib/lexer.js",
    "build-parser": "jison src/parser.y -o lib/parser.js",
    "clean": "rm -rf node_modules",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard",
    "pretest": "npm run lint && npm run build",
    "test": "nyc mocha"
  },
  "license": "MIT",
  "standard": {
    "globals": [
      "describe",
      "it"
    ],
    "ignore": [
      "lib/lexer.js",
      "lib/parser.js"
    ]
  }
}
